NEW TABLE user (
   userId INT = {nullable: false, default: 1, searchable: true, unique: true},
   userName CHAR[200] = {nullable: true, fk: employee.id},
   registered FLAG = {nullable: true, value: ["yes","no"]}
) primary = userId;

ALTER TABLE user (
   ADD COLUMN status FLAG = {nullable: false, value: ["offline","idle","active","error"]},
   EDIT COLUMN userName AS CHAR[128]
);


